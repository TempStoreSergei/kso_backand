[Unit]
Description=Cash System
After=network.target

[Service]
Type=simple
User=fsadmin
WorkingDirectory=/home/fsadmin/kso_modular_backend/devices/cash_system/devices_v1
ExecStart=/home/fsadmin/kso_modular_backend/devices/cash_system/devices_v1/.venv/bin/python /home/fsadmin/kso_modular_backend/devices/cash_system/devices_v1/run_queue_server.py
Restart=on-failure
Environment="PYTHONUNBUFFERED=1"

[Install]
WantedBy=multi-user.target
